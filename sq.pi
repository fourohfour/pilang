n : 100
i : 0

sq : [n - i : ( @s
                's'  : 0
                'c'  : 0 
                'sum': [i - 'c' : (@c
                                   's' : 's' + i
                                   'c' : 'c' + 1
                                  )
                ]
                i : i + 1
                ! : s
              )
     ]

! : sq
